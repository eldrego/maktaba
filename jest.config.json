{
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
      "node_modules",
      "<rootDir>/src/services/"
  ],
  "coverageDirectory": "<rootDir>/coverage/",
  "coverageThreshold": {
      "global": {
          "branches": 20,
          "functions": 30,
          "lines": 50,
          "statements": 50
      }
  },
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx",
    "ts",
    "tsx",
    "node"
  ],
  "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@ionic-native|@ionic|@ngrx|angular2-ui-switch|angularfire2|jest-cli|react-loader-spinner)"
  ],
  "verbose": true
}